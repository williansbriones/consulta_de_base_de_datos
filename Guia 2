--caso 1 

SELECT to_char(FECINI_ARRIENDO, 'DD/MM/YYYY') FROM PROPIEDAD_ARRENDADA
WHERE to_char(FECINI_ARRIENDO, 'DD/MM/YYYY') LIKE '______&fecha'
ORDER BY FECINI_ARRIENDO;
  
  
SELECT to_char(FECINI_ARRIENDO, 'DD/MM/YYYY') FROM PROPIEDAD_ARRENDADA 
WHERE to_char(FECINI_ARRIENDO, 'YYYY')='&fecha'
ORDER BY FECINI_ARRIENDO;

SELECT to_char(FECHA_ENTREGA_PROPIEDAD, 'DD/MM/YYYY') FROM propiedad
WHERE to_char(FECHA_ENTREGA_PROPIEDAD, 'YYYY')='&fecha'
ORDER BY FECHA_ENTREGA_PROPIEDAD;

--caso 2
SELECT NUMRUT_CLI ||'-'|| DVRUT_CLI AS "RUT",
NOMBRE_CLI ||' '|| APPATERNO_CLI ||' '|| APMATERNO_CLI AS "Nombre completo",
to_char(RENTA_CLI,'999g999g999') AS "renta",
FONOFIJO_CLI AS "telefono fijo",
CELULAR_CLI AS "Celular"
FROM CLIENTE
WHERE RENTA_CLI >= 800000 and (id_estcivil = 1 or id_estcivil = 3)
ORDER BY APPATERNO_CLI, NOMBRE_CLI;


